<launch>
    <!-- 1.需要在参数服务器当中载入 urdf -->
    <param name="robot_description" command="$(find xacro)/xacro $(find urdf02_gazebo)/urdf/car.urdf.xacro" />
    <!-- 2.启动Gazebo仿真环境 -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch" >
        <arg name="world_name" value="$(find urdf02_gazebo)/worlds/box_house_2.world" />
        <!-- true时打开3D可视化；false时关闭3D可视化 -->
        <arg name="gui" value="true" />
    </include >
    
    <!-- 3.在Gazebo中添加机器人模型 -->
    <node pkg="gazebo_ros" type="spawn_model" name="spawn_model" args="-urdf -model car -param robot_description"/>
    
    <!-- 4.启动椭圆运动动态障碍物控制器 -->
    <node pkg="urdf02_gazebo" type="elliptical_motion_controller.py" name="elliptical_motion_controller" output="screen" />
    
</launch>