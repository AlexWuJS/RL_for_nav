<sdf version='1.7'>
  <world name='default'>
    
    <!-- ================= 基础环境 ================= -->
    <light name='sun' type='directional'>
      <cast_shadows>1</cast_shadows>
      <pose>0 0 10 0 -0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>

    <model name='ground_plane'>
      <static>1</static>
      <link name='link'>
        <collision name='collision'>
          <geometry><plane><normal>0 0 1</normal><size>100 100</size></plane></geometry>
          <surface><friction><ode><mu>100</mu><mu2>50</mu2></ode></friction></surface>
        </collision>
        <visual name='visual'>
          <cast_shadows>0</cast_shadows>
          <geometry><plane><normal>0 0 1</normal><size>100 100</size></plane></geometry>
          <material><script><uri>file://media/materials/scripts/gazebo.material</uri><name>Gazebo/Grey</name></script></material>
        </visual>
      </link>
    </model>

    <gravity>0 0 -9.8</gravity>
    
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>0 -30 40 0 0.9 1.57</pose>
        <view_controller>orbit</view_controller>
      </camera>
    </gui>

    <!-- ================= 边界墙 (40m x 40m) ================= -->
    <model name='boundary_walls'>
      <static>1</static>
      <link name='walls'>
        <collision name='north'><pose>0 20 1 0 0 0</pose><geometry><box><size>40 0.5 2</size></box></geometry></collision>
        <visual name='north'><pose>0 20 1 0 0 0</pose><geometry><box><size>40 0.5 2</size></box></geometry><material><script><name>Gazebo/Bricks</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>

        <collision name='south'><pose>0 -20 1 0 0 0</pose><geometry><box><size>40 0.5 2</size></box></geometry></collision>
        <visual name='south'><pose>0 -20 1 0 0 0</pose><geometry><box><size>40 0.5 2</size></box></geometry><material><script><name>Gazebo/Bricks</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>

        <collision name='east'><pose>20 0 1 0 0 0</pose><geometry><box><size>0.5 40 2</size></box></geometry></collision>
        <visual name='east'><pose>20 0 1 0 0 0</pose><geometry><box><size>0.5 40 2</size></box></geometry><material><script><name>Gazebo/Bricks</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>

        <collision name='west'><pose>-20 0 1 0 0 0</pose><geometry><box><size>0.5 40 2</size></box></geometry></collision>
        <visual name='west'><pose>-20 0 1 0 0 0</pose><geometry><box><size>0.5 40 2</size></box></geometry><material><script><name>Gazebo/Bricks</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>
      </link>
    </model>

    <!-- ================= 静态障碍物 ================= -->
    <model name='static_box_1'>
      <pose>-5 -5 0.5 0 0 0</pose>
      <static>1</static>
      <link name='link'>
        <collision name='collision'><geometry><box><size>1 1 1</size></box></geometry></collision>
        <visual name='visual'><geometry><box><size>1 1 1</size></box></geometry><material><script><name>Gazebo/Grey</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>
      </link>
    </model>

    <!-- [新增] 静态障碍物 2: 圆柱体 -->
    <model name='static_cylinder_2'>
      <pose>8 5 0.5 0 0 0</pose>
      <static>1</static>
      <link name='link'>
        <collision name='collision'><geometry><cylinder><radius>0.5</radius><length>1</length></cylinder></geometry></collision>
        <visual name='visual'><geometry><cylinder><radius>0.5</radius><length>1</length></cylinder></geometry><material><script><name>Gazebo/Grey</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>
      </link>
    </model>

    <!-- [新增] 静态障碍物 3: 长条墙体 -->
    <model name='static_wall_3'>
      <pose>-15 -10 1 0 0 0</pose>
      <static>1</static>
      <link name='link'>
        <collision name='collision'><geometry><box><size>2 8 2</size></box></geometry></collision>
        <visual name='visual'><geometry><box><size>2 8 2</size></box></geometry><material><script><name>Gazebo/Grey</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>
      </link>
    </model>

    <!-- [新增] 静态障碍物 4: 正方体 -->
    <model name='static_box_4'>
      <pose>0 15 0.5 0 0 0</pose>
      <static>1</static>
      <link name='link'>
        <collision name='collision'><geometry><box><size>1.5 1.5 1</size></box></geometry></collision>
        <visual name='visual'><geometry><box><size>1.5 1.5 1</size></box></geometry><material><script><name>Gazebo/Grey</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material></visual>
      </link>
    </model>

    <!-- ================= 动态障碍物 (已改为 Model，等待 Python 控制) ================= -->
    
    <!-- 原红色方块 -->
    <model name="moving_box_square">
      <pose>5 5 0.6 0 0 0</pose>
      <static>0</static> <!-- 设置为非静态，允许移动 -->
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.16</ixx><iyy>0.16</iyy><izz>0.16</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><box><size>1.2 1.2 1.2</size></box></geometry>
          <material><script><name>Gazebo/Red</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><box><size>1.2 1.2 1.2</size></box></geometry></collision>
      </link>
    </model>

    <!-- 原蓝色方块 -->
    <model name="moving_box_patrol">
      <pose>-10 10 0.5 0 0 0</pose>
      <static>0</static>
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.16</ixx><iyy>0.16</iyy><izz>0.16</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><box><size>1 1 1</size></box></geometry>
          <material><script><name>Gazebo/Blue</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><box><size>1 1 1</size></box></geometry></collision>
      </link>
    </model>

    <!-- 原绿色圆柱 -->
    <model name="moving_cylinder_diamond">
      <pose>8 -8 0.6 0 0 0</pose>
      <static>0</static>
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.1</ixx><iyy>0.1</iyy><izz>0.1</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><cylinder><radius>0.6</radius><length>1.2</length></cylinder></geometry>
          <material><script><name>Gazebo/Green</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><cylinder><radius>0.6</radius><length>1.2</length></cylinder></geometry></collision>
      </link>
    </model>

        <!-- 原绿色圆柱 -->
    <model name="moving_cylinder_diamond_2">
      <pose>-8 8 0.6 0 0 0</pose>
      <static>0</static>
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.1</ixx><iyy>0.1</iyy><izz>0.1</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><cylinder><radius>0.3</radius><length>1.2</length></cylinder></geometry>
          <material><script><name>Gazebo/Green</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><cylinder><radius>0.6</radius><length>1.2</length></cylinder></geometry></collision>
      </link>
    </model>

    <!-- [新增] 动态障碍物 5: 黄色球体 -->
    <model name="moving_sphere_yellow">
      <pose>0 -10 0.5 0 0 0</pose>
      <static>0</static>
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.1</ixx><iyy>0.1</iyy><izz>0.1</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><sphere><radius>0.5</radius></sphere></geometry>
          <material><script><name>Gazebo/Yellow</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><sphere><radius>0.5</radius></sphere></geometry></collision>
      </link>
    </model>

    <!-- [新增] 动态障碍物 6: 紫色方块 -->
    <model name="moving_box_purple">
      <pose>-5 0 0.5 0 0 0</pose>
      <static>0</static>
      <link name="link">
        <inertial><mass>1.0</mass><inertia><ixx>0.16</ixx><iyy>0.16</iyy><izz>0.16</izz><ixy>0</ixy><ixz>0</ixz><iyz>0</iyz></inertia></inertial>
        <visual name="visual">
          <geometry><box><size>1 1 1</size></box></geometry>
          <material><script><name>Gazebo/Purple</name><uri>file://media/materials/scripts/gazebo.material</uri></script></material>
        </visual>
        <collision name="collision"><geometry><box><size>1 1 1</size></box></geometry></collision>
      </link>
    </model>


  </world>
</sdf>